generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:test.db"
}

model Application {
  id                Int        @id @default(autoincrement())
  primaryDriver     String?
  mailingAddress    String?
  garagingAddress   String?
  vehicles          String?
  additionalDrivers String?
  completed         Boolean    @default(false)
  quote             Int?
}
